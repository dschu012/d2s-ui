{
  "name": "d2s-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "tsc && vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "tsc": "tsc",
    "dev-server": "ts-node-dev --respawn --transpileOnly ./app.ts",
    "dev-ui": "webpack --config ./node_modules/@vue/cli-service/webpack.config.js --watch",
    "dev": "concurrently 'npm run dev-ui' 'npm run dev-server'"
  },
  "dependencies": {
    "core-js": "^3.4.3",
    "express": "4.17.1",
    "vue": "^2.6.10",
    "vuex": "3.1.2",
    "vue-router": "3.1.3",
    "dotenv": "8.2.0",
    "d2s": "dschu012/d2s#v1.0.0",
    "jimp": "0.9.3",
    "@jimp/png": "0.9.3",
    "tippy.js": "5.1.2",
    "glob": "7.1.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@types/express": "4.17.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10",
    "typescript": "^3.7.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "webpack-cli": "3.3.10",
    "concurrently": "5.0.0",
    "node-sass": "7.0.0",
    "sass-loader": "8.0.0"
  }
}
